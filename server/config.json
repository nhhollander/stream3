{
    "wsserver": {
        "host": "",
        "port": 4433,
        "certfile": "/internalcert/raspberrypi.internal.crumbcake.cc.pem",
        "keyfile": "/internalcert/raspberrypi.internal.crumbcake.cc.key"
    },
    "auth": {
        "keys": {
            "testkey": {
                "namepattern": ".*",
                "permissions": [
                    "message.media",
                    "media.play",
                    "media.pause"
                ],
                "admin": false
            },
            "adminkey": {
                "namepattern": "crumbcake",
                "permissions": ["*"],
                "admin": true
            }
        }
    },
    "client": {
        "default_message_duration": 5000
    },
    "media": {}
}